def isprime(n):
    if n==1:
        return False
    for i in range(2,n):
        if n%i==0:
            return False
    return True
n=int(input('enter value:'))
l=[]
c=0
for i in range(n):
    s=int(input())
    l.append(s)
print(l)
for i in range(len(l)):
    if isprime(l[i]):
        c=c+1
        print(l[i],end=' ')
        print()
print('count=',c)
